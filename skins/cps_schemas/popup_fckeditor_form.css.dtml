<dtml-comment>
$Id$
</dtml-comment>

<dtml-let last_modified="_.DateTime()-14" expires="_.DateTime()+1" >
<dtml-call "REQUEST.RESPONSE.setHeader( 'Content-Type', 'text/css' )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Last-Modified', last_modified.toZone('GMT').rfc822() )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Cache-Control', 'max-age=36000, must-revalidate' )">
<dtml-call "REQUEST.RESPONSE.setHeader( 'Expires', expires.toZone('GMT').rfc822() )" >
</dtml-let>

@import "<dtml-var expr="getBaseUrl()">editor/fck_skins/default/fck_dialog.css";

button {
  font-size: 11px;
  font-family: 'Microsoft Sans Serif' , Arial, Helvetica, Verdana, sans-serif;
}

button:hover {
  background-color: #d7d79f;
}
